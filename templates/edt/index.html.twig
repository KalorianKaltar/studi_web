{% extends 'base.html.twig' %}

{% block title %}EDT index{% endblock %}

{% block body %}
    <h1>EDT index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for e_d_t in e_d_ts %}
            <tr>
                <td>{{ e_d_t.id }}</td>
                <td>{{ e_d_t.date ? e_d_t.date|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_e_d_t_show', {'id': e_d_t.id}) }}">show</a>
                    <a href="{{ path('app_e_d_t_edit', {'id': e_d_t.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_e_d_t_new') }}">Create new</a>
{% endblock %}
