{% extends 'base.html.twig' %}

{% block title %}EDT{% endblock %}

{% block content %}
    <h1>EDT</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Date</th>
                <td>{{ e_d_t.date ? e_d_t.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Medecin</th>
                <td>Dr. {{ e_d_t.idMedecin.prenom }} {{ e_d_t.idMedecin.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adminIndex') }}">Retour</a>

    <a href="{{ path('app_e_d_t_edit', {'id': e_d_t.id}) }}">Editer</a>

{#    {{ include('edt/_delete_form.html.twig') }}#}
{% endblock %}
